{
  "name": "web-timer",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "0.1.0",
  "description": "Full screen timer",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "build": "parcel build --public-url '.'",
    "fmt": "prettier --write . --log-level error"
  },
  "pwaManifest": {
    "name": "Timer",
    "shortName": "Timer",
    "description": "Full screen timer",
    "startURL": "./?pwa=true",
    "theme": "#ffffff",
    "generateIconOptions": {
      "baseIcon": "./public/icon.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "keywords": [
    "timer"
  ],
  "author": "Tej Chajed",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.14.2",
    "@parcel/packager-raw-url": "2.14.2",
    "@parcel/packager-xml": "2.14.2",
    "@parcel/transformer-less": "^2.14.2",
    "@parcel/transformer-raw": "^2.14.2",
    "parcel": "^2.14.2",
    "parcel-config-pwa-manifest": "^0.1.6",
    "prettier": "^3.5.3"
  },
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
