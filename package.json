{
  "name": "web-timer",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "0.1.0",
  "description": "Full screen timer",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "build": "parcel build --public-url '.'",
    "fmt": "prettier --write . --log-level error"
  },
  "pwaManifest": {
    "name": "Timer",
    "shortName": "Timer",
    "description": "Full screen timer",
    "startURL": "./?pwa=true",
    "theme": "#ffffff",
    "generateIconOptions": {
      "baseIcon": "./public/icon.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "keywords": [
    "timer"
  ],
  "author": "Tej Chajed",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.15.1",
    "@parcel/packager-raw-url": "2.15.1",
    "@parcel/packager-xml": "2.15.1",
    "@parcel/transformer-less": "^2.15.0",
    "@parcel/transformer-raw": "^2.15.0",
    "parcel": "^2.15.0",
    "parcel-config-pwa-manifest": "^0.1.6",
    "prettier": "^3.5.3"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
