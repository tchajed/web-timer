{
  "name": "web-timer",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "0.1.0",
  "description": "Full screen timer",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "build": "parcel build --public-url '.'",
    "fmt": "prettier --write . --log-level error"
  },
  "pwaManifest": {
    "name": "Timer",
    "shortName": "Timer",
    "description": "Full screen timer",
    "startURL": "./?pwa=true",
    "theme": "#ffffff",
    "generateIconOptions": {
      "baseIcon": "./public/icon.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "keywords": [
    "timer"
  ],
  "author": "Tej Chajed",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.15.4",
    "@parcel/packager-raw-url": "^2.15.4",
    "@parcel/packager-xml": "2.15.4",
    "@parcel/transformer-less": "^2.15.4",
    "@parcel/transformer-raw": "^2.15.4",
    "parcel": "^2.15.4",
    "parcel-config-pwa-manifest": "^0.1.6",
    "prettier": "^3.6.0"
  },
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
