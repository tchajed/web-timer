{
  "name": "web-timer",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "0.1.0",
  "description": "Full screen timer",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "build": "parcel build --public-url '.'",
    "fmt": "prettier --write . --log-level error"
  },
  "pwaManifest": {
    "name": "Timer",
    "shortName": "Timer",
    "description": "Full screen timer",
    "startURL": "./?pwa=true",
    "theme": "#ffffff",
    "generateIconOptions": {
      "baseIcon": "./public/icon.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "keywords": [
    "timer"
  ],
  "author": "Tej Chajed",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.15.2",
    "@parcel/packager-raw-url": "2.15.2",
    "@parcel/packager-xml": "2.15.2",
    "@parcel/transformer-less": "^2.15.1",
    "@parcel/transformer-raw": "^2.15.1",
    "parcel": "^2.15.1",
    "parcel-config-pwa-manifest": "^0.1.6",
    "prettier": "^3.5.3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
