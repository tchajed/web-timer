{
  "name": "web-timer",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "version": "0.1.0",
  "description": "Full screen timer",
  "scripts": {
    "dev": "parcel serve src/index.html",
    "build": "parcel build --public-url '.'",
    "fmt": "prettier --write . --log-level error"
  },
  "pwaManifest": {
    "name": "Timer",
    "shortName": "Timer",
    "description": "Full screen timer",
    "startURL": "./?pwa=true",
    "theme": "#ffffff",
    "generateIconOptions": {
      "baseIcon": "./public/icon.png",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  },
  "keywords": [
    "timer"
  ],
  "author": "Tej Chajed",
  "license": "MIT",
  "devDependencies": {
    "@parcel/config-default": "^2.15.4",
    "@parcel/packager-raw-url": "^2.15.4",
    "@parcel/packager-xml": "2.15.4",
    "@parcel/transformer-less": "^2.15.4",
    "@parcel/transformer-raw": "^2.15.4",
    "parcel": "^2.15.4",
    "parcel-config-pwa-manifest": "^0.1.6",
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.16.1+sha512.0e155aa2629db8672b49e8475da6226aa4bdea85fdcdfdc15350874946d4f3c91faaf64cbdc4a5d1ab8002f473d5c3fcedcd197989cf0390f9badd3c04678706",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ]
  }
}
